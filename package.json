{
  "name": "violetvault",
  "private": true,
  "version": "1.4.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --config ./configs/eslint.config.js --max-warnings 50",
    "lint:fix": "eslint . --config ./configs/eslint.config.js --fix",
    "format": "prettier --config ./configs/prettier.config.js --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --config ./configs/prettier.config.js --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "preview": "vite preview",
    "prepare": "husky",
    "commit": "git add . && git commit",
    "check-commit": "commitlint --config ./configs/commitlint.config.cjs --from HEAD~1 --to HEAD --verbose"
  },
  "dependencies": {
    "@sentry/react": "^9.41.0",
    "@sentry/vite-plugin": "^4.0.0",
    "@tailwindcss/vite": "^4.1.11",
    "firebase": "^12.0.0",
    "lightningcss": "^1.30.1",
    "lucide-react": "^0.525.0",
    "@tanstack/react-query": "^5.29.0",
    "dexie": "^3.2.5",
    "zustand": "^4.5.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^3.1.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "vite": "^7.0.6"
  }
}
