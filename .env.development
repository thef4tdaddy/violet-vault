# Development Environment Configuration
# VioletVault Development Settings

# App Environment
VITE_APP_ENV=development
VITE_APP_VERSION=1.6.1-dev

# Error Tracking & Analytics (Development)
VITE_HIGHLIGHT_PROJECT_ID=your-dev-project-id-here
VITE_SENTRY_DSN=your-dev-sentry-dsn-here

# Debug Settings
VITE_DEBUG_MODE=true
VITE_VERBOSE_LOGGING=true

# Development Features
VITE_ENABLE_DEV_TOOLS=true
VITE_SKIP_AUTH_IN_DEV=false

# Firebase (Development)
# Configure separate Firebase project for development if needed
# VITE_FIREBASE_PROJECT_ID=violet-vault-dev