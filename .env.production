# Production Environment Configuration
# VioletVault Production Settings

# App Environment
VITE_APP_ENV=production
VITE_APP_VERSION=1.6.1

# Error Tracking & Analytics (Production)
# LogRocket configuration for production environment
VITE_LOGROCKET_APP_ID=your-prod-app-id-here

# Error Reporting Configuration
VITE_ERROR_REPORTING_ENABLED=true
VITE_ERROR_SAMPLE_RATE=1.0
VITE_SESSION_SAMPLE_RATE=0.1

# Legacy (to be removed after migration)
VITE_HIGHLIGHT_PROJECT_ID=your-prod-project-id-here
VITE_SENTRY_DSN=your-prod-sentry-dsn-here

# Debug Settings (Disabled in production)
VITE_DEBUG_MODE=false
VITE_VERBOSE_LOGGING=false

# Production Features
VITE_ENABLE_DEV_TOOLS=false
VITE_SKIP_AUTH_IN_DEV=false

# Performance Settings
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true