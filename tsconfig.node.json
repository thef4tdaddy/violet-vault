{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "noEmit": true,
    "types": ["node"]
  },
  "include": [
    "vite.config.js",
    "vitest.config.js",
    "configs/**/*",
    "scripts/**/*"
  ]
}