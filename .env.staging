# Staging Environment Configuration
# VioletVault Staging Settings (for testing before production)

# App Environment
VITE_APP_ENV=staging
VITE_APP_VERSION=2.0.0-staging

# Error Tracking & Analytics (Staging)
VITE_HIGHLIGHT_PROJECT_ID=your-staging-project-id-here
VITE_ERROR_REPORTING_ENABLED=true
VITE_ERROR_SAMPLE_RATE=1.0
VITE_SESSION_SAMPLE_RATE=1.0

# Debug Settings (Enabled for testing)
VITE_DEBUG_MODE=true
VITE_VERBOSE_LOGGING=true

# Staging Features
VITE_ENABLE_DEV_TOOLS=true
VITE_SKIP_AUTH_IN_DEV=false

# Testing Settings
VITE_ENABLE_ANALYTICS=true

# Firebase Configuration (Staging) - PLEASE REPLACE WITH ACTUAL VALUES
VITE_FIREBASE_API_KEY=replace-with-staging-api-key
VITE_FIREBASE_AUTH_DOMAIN=replace-with-staging-auth-domain
VITE_FIREBASE_PROJECT_ID=replace-with-staging-project-id
VITE_FIREBASE_STORAGE_BUCKET=replace-with-staging-storage-bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=replace-with-staging-sender-id
VITE_FIREBASE_APP_ID=replace-with-staging-app-id
VITE_FIREBASE_MEASUREMENT_ID=replace-with-staging-measurement-id